// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// schema.prisma

// Define o modelo de dados para os estudantes
model Estudante {
  ra        String   @id
  nome      String
  curso     String
  fotoUrl   String
}